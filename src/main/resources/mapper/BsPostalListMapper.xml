        <result column="city" property="city" />
        <result column="district" property="district" />
        <result column="address" property="address" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="postal_company_id" property="postalCompanyId" />
        <result column="express_no" property="expressNo" />
        <result column="del_flag" property="delFlag" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, batch_no AS batchNo, receive_name AS receiveName, receive_tel AS receiveTel, province, city, district, address, create_time AS createTime, update_time AS updateTime, postal_company_id AS postalCompanyId, express_no AS expressNo, del_flag AS delFlag
