<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tan.ec.mapper.BsActivePersonMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.tan.ec.entity.BsActivePerson">
        <id column="id" property="id" />
        <result column="batch_no" property="batchNo" />
        <result column="name" property="name" />
        <result column="company" property="company" />
        <result column="job" property="job" />
        <result column="phone" property="phone" />
        <result column="email" property="email" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="del_flag" property="delFlag" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, batch_no AS batchNo, name, company, job, phone, email, create_time AS createTime, update_time AS updateTime, del_flag AS delFlag
    </sql>

</mapper>
